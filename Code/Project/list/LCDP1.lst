N/*********************************************************************
N*
N*       LCD_Color2Index_1
N*/
Nunsigned LCD_Color2Index_1(LCD_COLOR Color) {
N  int r,g,b;
N  r = Color      &255;
N  g = (Color>>8) &255;
N  b = Color>>16;
N  return (r+g+b+383) /(3*255);
N}
N
N/*********************************************************************
N*
N*       LCD_Index2Color_1
N*/
NLCD_COLOR LCD_Index2Color_1(int Index) {
N  return Index ? 0xFFFFFF : 0;
N}
